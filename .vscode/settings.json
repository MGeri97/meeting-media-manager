    "español",
    "icns",
    "Italiano",
    "jekyll",
    "JWMMF",
    "jworg",
    "koofr",
    "Koofr",
    "lffi",
    "Lffi",
    "meps",
    "mwbr",
    "popperjs",
    "Português",
    "roboto",
    "sircharlo",
    "sjjm",
    "snnw",
    "Språk",
    "sqljs",
    "suomi",
    "uncompiled",
    "unmute",
    "upath",
    "vetur",
    "vuedraggable",
    "webdav",
    "wtlocale",
    "xpda",
