{
  "editor.formatOnSave": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "cSpell.ignorePaths": [
    "package-lock.json",
    "node_modules",
    "vscode-extension",
    ".git/objects",
    ".vscode",
    ".vscode-insiders",
    "docs/assets.js"
  ],
  "cSpell.ignoreWords": ["bootstrap", "-sm"],
  "cSpell.words": [
    "-sm",
    "americas",
    "bootstrap",
    "Creds",
    "dayjs",
    "docid",
    "flatpickr",
    "islocal",
    "isnotpresentonremote",
    "jwmmf",
    "jworg",
    "lffi",
    "mbps",
    "Meps",
    "MKCOL",
    "mwbr",
    "sjjm",
    "snnw",
    "sqljs",
    "unconfirm",
    "upath",
    "webdav",
    "xspf"
  ]
}
