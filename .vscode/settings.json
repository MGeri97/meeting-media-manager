{
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "cSpell.ignorePaths": [
    "package-lock.json",
    "node_modules",
    "vscode-extension",
    ".git/objects",
    ".vscode",
    ".vscode-insiders",
    "docs/assets.js"
  ],
  "cSpell.ignoreWords": [
    "bootstrap",
    "-sm"
  ],
  "cSpell.words": [
    "-sm",
    "americas",
    "Anonymized",
    "aspectratio",
    "autohide",
    "bootstrap",
    "Creds",
    "datetime",
    "datevalue",
    "dayjs",
    "docid",
    "dont",
    "dryrun",
    "fileformat",
    "filesize",
    "flatpickr",
    "fontawesome",
    "fortawesome",
    "getcontentlength",
    "inputmask",
    "islocal",
    "isnotpresentonremote",
    "jwmmf",
    "jworg",
    "jwpub",
    "langcode",
    "langwritten",
    "lffi",
    "localpath",
    "mbps",
    "Meps",
    "MKCOL",
    "multistatus",
    "mwbr",
    "nsis",
    "popperjs",
    "prefs",
    "progressbar",
    "PROPFIND",
    "propstat",
    "resourcetype",
    "safename",
    "settingslink",
    "sircharlo",
    "sjjm",
    "snnw",
    "sqljs",
    "tabindex",
    "timeinterval",
    "timestart",
    "unconfirm",
    "upath",
    "vals",
    "webdav",
    "xspf",
    "YEARTEXT"
  ],
  "files.autoSave": "onFocusChange",
  "editor.tabSize": 2,
  "editor.formatOnPaste": true,
  "editor.formatOnType": true,
  "diffEditor.codeLens": true,
  "diffEditor.ignoreTrimWhitespace": true,
  "window.autoDetectColorScheme": false
}