{
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.fixAll.markdownlint": true
  },
  "cSpell.words": [
    "americas",
    "booknum",
    "codeql",
    "congs",
    "corejs",
    "dayjs",
    "Descr",
    "docid",
    "dont",
    "EPUB",
    "esnext",
    "icns",
    "jekyll",
    "JWMMF",
    "jworg",
    "koofr",
    "Koofr",
    "lffi",
    "meps",
    "mwbr",
    "sircharlo",
    "sjjm",
    "snnw",
    "sqljs",
    "uncompiled",
    "unmute",
    "upath",
    "vetur",
    "vuedraggable",
    "webdav",
    "wtlocale",
    "xpda",
    "xspf"
  ],
  "cSpell.ignoreWords": ["transform", "PDF", "TERMS"],
  "cSpell.ignorePaths": ["src/renderer/locales/*", "docs/_data/*"]
}
